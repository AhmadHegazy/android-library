<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".SecondActivity" 
    tools:ignore="HardcodedText">

    <TextView
        android:id="@+id/activitysecond_label_progress"
    	android:layout_width="wrap_content"
	    android:layout_height="wrap_content"
	    android:textSize="16sp"
	    android:text="0"
	    android:layout_marginLeft="10dp"
	    android:layout_alignParentTop="true"
	    android:layout_alignParentRight="true"/>
    
	<SeekBar
	    android:id="@+id/activitysecond_seekbar"
	    android:layout_width="0dp"
	    android:layout_height="wrap_content"
	    android:layout_alignParentTop="true"
	    android:layout_alignBottom="@id/activitysecond_label_progress"
	    android:layout_alignParentLeft="true"
	    android:layout_toLeftOf="@id/activitysecond_label_progress"
	    android:gravity="center"/>
	
	<com.tealium.example.compact.controls.DispatchListView
        android:id="@+id/activitymain_disptachlistview" 
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:layout_below="@id/activitysecond_label_progress"
        android:layout_alignParentLeft="true"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"/>
    
</RelativeLayout>
